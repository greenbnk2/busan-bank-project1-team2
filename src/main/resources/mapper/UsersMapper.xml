<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.bnkfirst.mapper.UsersMapper">

    <!-- 아이디로 회원 조회 -->
    <select id="findUserById" parameterType="string" resultType="kr.co.bnkfirst.dto.UsersDTO">
        SELECT
            MID AS mid,
            MPW AS mpw
        FROM USERS
        WHERE MID = #{mid}
    </select>

</mapper>
